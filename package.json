{
  "name": "express-ci-cd-harness",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/app.js --exec \"node\"",
    "dev-reload": "nodemon src/app.js --exec \"npm run lint && node\"",
    "start": "node src/app.js",
    "prod": "NODE_ENV=production node --optimize_for_size --trace-warnings --no-warnings src/app.js",
    "prod-pm2": "NODE_ENV=production pm2 start src/app.js",
    "lint": "eslint --ext .js src/",
    "lint:fix": "eslint --ext .js src/ --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "express-status-monitor": "^1.3.4",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.2",
    "node-toobusy": "^0.6.3",
    "pm2": "^5.3.0",
    "response-time": "^2.3.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1"
  }
